<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads FreeMind dmg from sourceforge.
By default downloads latest version</string>
	<key>Identifier</key>
	<string>com.github.prochat.FreeMind.download</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>FreeMind</string>
       	<key>DOWNLOAD_URL</key>
       	<string>https://sourceforge.net/projects/freemind/files/</string>
       	<key>SEARCH_PATTERN_URL</key>
		<string>href="(?P&lt;url&gt;.*\.dmg.*)"</string>
		<key>SEARCH_PATTERN_VERSION</key>
		<string>href=".*title="/(?P&lt;url2&gt;[\.0-9]*).*/.*\.dmg.*"</string>
	</dict>
	<key>Process</key>
	<array>
	 	<dict>
			<key>Processor</key>
           		<string>URLTextSearcher</string>
           		<key>Arguments</key>
            		<dict>
                		<key>url</key>
                		<string>%DOWNLOAD_URL%</string>
                		<key>re_pattern</key>
                		<string>%SEARCH_PATTERN_URL%</string>
            		</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>URLDownloader</string>
			<key>Arguments</key>
			<dict>
				<key>url</key>
				<string>http://sourceforge.net%match%</string>
				<key>filename</key>
				<string>%NAME%.tar.gz</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
           		<string>URLTextSearcher</string>
           		<key>Arguments</key>
            		<dict>
                		<key>url</key>
                		<string>%DOWNLOAD_URL%</string>
						<key>result_output_var_name</key>
						<string>version</string>
                		<key>re_pattern</key>
                		<string>%SEARCH_PATTERN_VERSION%</string>
            		</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
	</array>
</dict>
</plist>
